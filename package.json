{
	"name"           :"icx-compiler",
	"version"        :"1.3.9",
	"description"    :"Compiler icX to ic10 ",
	"main"           :"index.js",
	"scripts"        :{
		"build" :"babel src -d src",
		"whatch":"tsc -w",
		"test"  :"node test.js",
		"jest"  :"jest tests/jest.js",
		"tests" :"node test-run.js",
		"debug" :"nodemon -w ./ -e js,icX jest"
	},
	"transform"      :{
		"\\.js$":"<rootDir>/node_modules/babel-jest"
	},
	"homepage"       :"https://github.com/Traineratwot/icX",
	"keywords"       :[
		"ic10",
		"icX",
		"compiller"
	],
	"files"          :[
		"src/*",
		"index.ts",
		"index.d.ts",
		"index.js",
		"index.js.map",
		"test.ts"
	],
	"exclude"        :[
		".idea",
		".vscode",
		".ic10",
		".icX",
		"tests"
	],
	"module"         :"commonjs",
	"author"         :"Traineratwot",
	"license"        :"MIT",
	"dependencies"   :{
		"@scicave/math-parser":"^5.1.0"
	},
	"devDependencies":{
		"@jest/globals":"^29.3.1",
		"@types/jest"  :"^29.2.4",
		"@types/node"  :"^15.14.9",
		"ic10"         :"^2.0.0",
		"jest"         :"^29.3.1",
		"ts-jest"      :"^29.0.3",
		"typescript"   :"^4.9.3"
	}
}
