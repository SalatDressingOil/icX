alias this db
alias MaxTempSet d0
alias MinTempSet d1
alias MaxPressureSet d2
alias MinPressureSet d3
alias LedError d5

var temp
var tempOut
var maxTemp
var minTemp
var maxPressure
var minPressure

LedError.On = 1
main:
    minPressure = MinPressureSet.Setting
    minPressure = minPressure + 100
    push minPressure

    maxPressure = MaxPressureSet.Setting
    maxPressure = maxPressure - 100
    push maxPressure
    
    minTemp = MinTempSet.Setting
    minTemp = minTemp + 5
    push minTemp

    maxTemp = MaxTempSet.Setting
    maxTemp = maxTemp - 5
    push maxTemp

    if minTemp >= 350 && minPressure >= 100 && minTemp < maxTemp && minPressure < maxPressure
        LedError.Color = 2
        temp = this.Setting
        if temp <= 0
            sp = sp + temp
            peek tempOut
            this.Setting = tempOut
            sp = sp - temp
        end
    else
        LedError.Color = 4
    end

    pop temp
    pop temp
    pop temp
    pop temp
j main